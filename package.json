    "@solana/web3.js": "^1.66.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/react-slider": "^1.3.1",
    "axios": "^0.21.2",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-plugin-twin": "^1.0.2",
    "bn.js": "^5.2.1",
    "borsh": "^0.7.0",
