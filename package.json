{
  "name": "acs-widget",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build-release": "webpack --config webpack.config.js --env.prod --display-modules",
    "start": "NODE_ENV=development webpack serve",
    "lint": "tslint --project tsconfig.json ./src/**/*.tsx",
    "lint-fix": "tslint --project tsconfig.json ./src/**/*.tsx --fix",
    "test": "jest",
    "prod:stats": "webpack --mode production --profile --json > stats.json"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@statoscope/webpack-plugin": "^5.24.0",
    "@types/jest": "^25.2.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "jest": "^29.0.0",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^8.0.0",
    "ts-jest": "^29.0.3",
    "tslint": "^5.20.1",
